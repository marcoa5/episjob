{
  "name": "ServiceJob",
  "version": "1.6.1",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build": "electron-builder build --win --publish never",
    "deploy": "electron-builder build --win --publish always"
  },
  "build": {
    "asar": false,
    "appId": "sjma",
    "mac": {
      "category": "your.app.category.type"
    },
    "files": [
      "**/*",
      "!molupd.txt",
      "!customersupd.txt",
      "!techupd.txt"
    ],
    "fileAssociations": [
      {
        "ext": "ma",
        "role": "Editor",
        "icon": "img/docicon.ico",
        "name": "Service Job File"
      }
    ]
  },
  "author": "Marco A",
  "license": "ISC",
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-builder": "^22.10.3",
    "electron-packager": "^15.2.0",
    "electron-rebuild": "^1.10.1",
    "electron-winstaller": "^4.0.0",
    "electron-wix-msi": "^2.2.0"
  },
  "dependencies": {
    "asar": "^2.0.3",
    "dotenv": "^8.2.0",
    "electron-download-manager": "^2.1.2",
    "electron-prompt": "^1.4.0",
    "electron-updater": "^4.2.0",
    "firebase": "^8.2.1",
    "fs-extra": "^8.1.0",
    "handlebars": "^4.7.6",
    "mailgun-js": "^0.22.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.26.0",
    "node": "^13.5.0",
    "nodemailer": "^6.4.17",
    "nodemailer-mailgun-transport": "^2.0.2",
    "sprestlib": "^1.9.0",
    "websocket-extensions": "^0.1.4"
  },
  "description": "service job",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcoa5/episjob.git"
  }
}
